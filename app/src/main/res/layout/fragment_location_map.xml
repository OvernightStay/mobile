<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.dating.DatingFragment">


    <ImageView
        android:id="@+id/location_map"
        android:layout_width="800dp"
        android:layout_height="360dp"
        android:importantForAccessibility="no"
        android:src="@drawable/bg_location_map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/night_bus"
        android:layout_width="123dp"
        android:layout_height="78dp"
        android:layout_marginTop="267dp"
        android:layout_marginStart="260dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_night_bus"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_shower"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="221dp"
        android:layout_marginStart="125dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_shower"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_laundry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="164dp"
        android:layout_marginStart="237dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_laundry"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_nursing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:layout_marginStart="210dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_nursing"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_rehabilitation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:layout_marginStart="317dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_rehabilitation"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_consultation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="186dp"
        android:layout_marginStart="353dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_consultation"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_halfpath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:layout_marginStart="506dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_halfpath"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_warm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="278dp"
        android:layout_marginStart="380dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_warm"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_of_distribution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="210dp"
        android:layout_marginStart="487dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_of_distribution"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_night"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="141dp"
        android:layout_marginStart="429dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_night"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/img_house_psychologist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="264dp"
        android:layout_marginStart="526dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_house_psychologist"
        android:alpha="0"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/location_map"
        app:layout_constraintTop_toTopOf="@id/location_map"/>

    <ImageView
        android:id="@+id/home"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="14dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_home"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/stress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="28dp"
        android:importantForAccessibility="no"
        android:src="@drawable/img_stress"
        app:layout_constraintStart_toEndOf="@id/home"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gear_wheel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="14dp"
        android:background="@drawable/img_gear_wheel"
        android:importantForAccessibility="no"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/backpack"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/img_backpack"
        android:importantForAccessibility="no"
        app:layout_constraintEnd_toStartOf="@id/gear_wheel"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/rules"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/img_rules"
        android:importantForAccessibility="no"
        app:layout_constraintEnd_toStartOf="@id/backpack"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cat_avatar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/img_cat_avatar"
        android:importantForAccessibility="no"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/gear_wheel" />

    <ImageView
        android:id="@+id/map"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/img_map"
        android:importantForAccessibility="no"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cat_avatar" />

    <ImageView
        android:id="@+id/cat_status"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:background="@drawable/img_cat_status"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/rectangle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="14dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rectangle_100"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/status_name"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/user_name"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:text="Статус"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle" />

    <ImageView
        android:id="@+id/dialog_next"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="35dp"
        android:background="@drawable/dating_dialog_next"
        android:importantForAccessibility="no"
        android:src="@drawable/dating_dialog_next"
        android:stateListAnimator="@animator/anim_btn_next"
        android:textColorHint="#A5D38D"
        app:layout_constraintBottom_toBottomOf="@id/rectangle"
        app:layout_constraintEnd_toEndOf="@id/rectangle" />

    <TextView
        android:id="@+id/text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:text="@string/cat_status"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/dialog_next"
        app:layout_constraintStart_toStartOf="@+id/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle" />


</androidx.constraintlayout.widget.ConstraintLayout>