<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="PrimaryBigButton" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">288dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@drawable/bg_primary_big_button</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ModalButtonGreen" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/background_modal_button_green</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">sans-serif</item>
        <!-- Другие стили для кнопки -->
    </style>

    <style name="ModalButtonYellow" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/background_modal_button_yellow</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">sans-serif</item>
        <!-- Другие стили для кнопки -->
    </style>
    <style name="MemoCardBack">
        <item name="android:background">@drawable/background_memo_backcard</item>
    </style>

    <style name="MemoCardBackStyle">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:background">@drawable/background_memo_backcard</item>
    </style>

    <!-- Стили для text -->

    <style name="text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginTop">50dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:text">@string/cat_status</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- Стили для status_name -->

    <style name="status_name">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/user_name</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <!-- Стили для user_name -->

    <style name="user_name">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:background">@drawable/user_name</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <!-- Стили для dialog_next -->

    <style name="dialog_next">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:layout_marginBottom">35dp</item>
        <item name="android:background">@drawable/dating_dialog_next</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:src">@drawable/dating_dialog_next</item>
        <item name="android:stateListAnimator">@animator/anim_btn_next</item>
    </style>

    <!-- Стили для rectangle -->

    <style name="rectangle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">132dp</item>
        <item name="android:layout_margin">14dp</item>
    </style>

    <!-- Стили для cat_status -->

    <style name="cat_status">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">14dp</item>
        <item name="android:layout_marginBottom">14dp</item>
        <item name="android:src">@drawable/img_cat_status</item>
    </style>

    <!-- Стили для home -->

    <style name="home">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">14dp</item>
        <item name="android:src">@drawable/img_home</item>
        <item name="android:stateListAnimator">@animator/anim_image_click</item>
    </style>

    <!-- Стили для stress -->

    <style name="stress">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">14dp</item>
        <item name="android:src">@drawable/img_stress</item>
    </style>

    <!-- Стили для rules -->

    <style name="rules">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:layout_marginEnd">28dp</item>
        <item name="android:src">@drawable/img_rules</item>
        <item name="android:stateListAnimator">@animator/anim_image_click</item>
    </style>

    <!-- Стили для backpack -->

    <style name="backpack">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">14dp</item>
        <item name="android:src">@drawable/img_backpack</item>
        <item name="android:stateListAnimator">@animator/anim_image_click</item>
    </style>

    <!-- Стили для gear_wheel -->

    <style name="gear_wheel">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">14dp</item>
        <item name="android:src">@drawable/img_gear_wheel</item>
    </style>

    <!-- Стили для cat-avatar -->

    <style name="cat_avatar">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:src">@drawable/img_cat_avatar</item>
    </style>

    <!-- Стили для map -->

    <style name="map">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:src">@drawable/img_map</item>
        <item name="android:stateListAnimator">@animator/anim_image_click</item>
    </style>

    <!-- Стили для back_arrow -->

    <style name="back_arrow">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:src">@drawable/img_back_arrow</item>
        <item name="android:stateListAnimator">@animator/anim_image_click</item>
    </style>

